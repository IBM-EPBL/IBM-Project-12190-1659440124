@charset "utf-8";

body { font-family: "IBM Plex Sans", serif; }

body .container { position: relative; border: 1px solid rgb(23, 45, 50); padding: 10px; box-shadow: rgb(85, 85, 85) 0px 0px 2px; max-width: 73.125rem; }

.cert { padding: 54px 54px 27px; border: 1px solid rgb(23, 45, 50); box-shadow: rgb(85, 85, 85) 0px 0px 2px inset; color: rgb(104, 106, 108); }

.cert strong { color: black; }

.cert h1, .cert h2 { color: black; }

.cert a { font-weight: 600; color: rgb(62, 131, 168); text-decoration: none; }

.wrapper { width: 100%; height: 100px; }

#logo, #ibm_logo { float: left; max-height: 85px; max-width: 100%; margin-bottom: 40px; }

#qr_code { float: right; padding: 5px; border: 1px solid black; background: white; margin: auto; position: relative; top: -8px; height: 100px; width: 100px; }

#course_list { text-align: left; position: absolute; top: 50%; transform: translateY(-65%); right: 16px; max-width: 150px; }

#course_list li { color: black; font-size: 0.9em; line-height: 1.5; }

#course_list div { margin-left: -24px; margin-bottom: 12px; }

.details { position: relative; clear: both; background: rgba(238, 238, 238, 0.6); text-align: center; padding: 48px 54px 64px 48px; margin: 0px -54px; }

.details h1, .details h2 { font-weight: 500; }

.details h3, .details h4, .details h5, .details h6 { font-weight: normal; }

.footer { display: flex; text-align: center; justify-content: space-between; }

.footer p { margin-bottom: 4px; margin-top: 4px; }

.footer h3 { font-weight: 400; margin-bottom: 6px; }

.footer h2 { margin-top: 6px; }

.cert-corner.cert-corner-tl { position: absolute; height: 14px; width: 14px; background-color: rgb(255, 255, 255); top: 10px; border-bottom: 1px solid rgb(23, 45, 50); left: 10px; border-right: 1px solid rgb(23, 45, 50); box-shadow: rgb(187, 187, 187) 1px 1px 1px; }

.cert-corner.cert-corner-tl::before { content: ""; position: absolute; width: 7px; height: 7px; border: 1px solid rgb(23, 45, 50); box-shadow: rgb(85, 85, 85) 0px 0px 2px inset; top: 0px; left: 0px; }

.cert-corner.cert-corner-tr { position: absolute; height: 14px; width: 14px; background-color: rgb(255, 255, 255); top: 10px; border-bottom: 1px solid rgb(23, 45, 50); right: 10px; border-left: 1px solid rgb(23, 45, 50); box-shadow: rgb(187, 187, 187) -1px 1px 1px; }

.cert-corner.cert-corner-tr::before { content: ""; position: absolute; width: 7px; height: 7px; border: 1px solid rgb(23, 45, 50); box-shadow: rgb(85, 85, 85) 0px 0px 2px inset; top: 0px; right: 0px; }

.cert-corner.cert-corner-bl { position: absolute; height: 14px; width: 14px; background-color: rgb(255, 255, 255); bottom: 10px; border-top: 1px solid rgb(23, 45, 50); left: 10px; border-right: 1px solid rgb(23, 45, 50); box-shadow: rgb(187, 187, 187) 1px -1px 1px; }

.cert-corner.cert-corner-bl::before { content: ""; position: absolute; width: 7px; height: 7px; border: 1px solid rgb(23, 45, 50); box-shadow: rgb(85, 85, 85) 0px 0px 2px inset; bottom: 0px; left: 0px; }

.cert-corner.cert-corner-br { position: absolute; height: 14px; width: 14px; background-color: rgb(255, 255, 255); bottom: 10px; border-top: 1px solid rgb(23, 45, 50); right: 10px; border-left: 1px solid rgb(23, 45, 50); box-shadow: rgb(187, 187, 187) -1px -1px 1px; }

.cert-corner.cert-corner-br::before { content: ""; position: absolute; width: 7px; height: 7px; border: 1px solid rgb(23, 45, 50); box-shadow: rgb(85, 85, 85) 0px 0px 2px inset; bottom: 0px; right: 0px; }

.signature { color: black; font-size: 12px; text-align: left; font-weight: 400; line-height: 1.5; display: flex; flex-direction: column; justify-content: flex-end; }

.signature img { width: 128px; height: auto; margin-bottom: 12px; }

@media (max-width: 815px) {
  #course_list { display: none; }
}
